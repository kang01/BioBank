<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <property name="now" value="now()" dbms="h2"/>

    <property name="now" value="sysdate" dbms="oracle"/>
    <property name="autoIncrement" value="true" dbms="mysql,h2,postgresql,oracle,mssql"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle, mssql"/>

    <!--
        Added the entity Tranship.
    -->
    <changeSet id="20170317065947-1" author="jhipster">
        <createTable tableName="tranship" remarks="转运表">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="tranship_date" type="date" remarks="转运日期">
                <constraints nullable="true" />
            </column>

            <column name="project_code" type="varchar(100)" remarks="项目编码">
                <constraints nullable="true" />
            </column>

            <column name="project_name" type="varchar(255)" remarks="项目名称">
                <constraints nullable="true" />
            </column>

            <column name="project_site_code" type="varchar(100)" remarks="项目点编码">
                <constraints nullable="true" />
            </column>

            <column name="project_site_name" type="varchar(255)" remarks="项目点名称">
                <constraints nullable="true" />
            </column>

            <column name="track_number" type="varchar(100)" remarks="运单号">
                <constraints nullable="true" />
            </column>

            <column name="tranship_batch" type="varchar(100)" remarks="批次">
                <constraints nullable="true" />
            </column>

            <column name="tranship_state" type="varchar(20)" remarks="转运状态：1001：进行中，1002：待入库，1003：已入库，1004：已作废">
                <constraints nullable="false" />
            </column>

            <column name="receiver" type="varchar(100)" remarks="接收人">
                <constraints nullable="true" />
            </column>

            <column name="receive_date" type="date" remarks="接受日期">
                <constraints nullable="true" />
            </column>

            <column name="sample_number" type="integer" remarks="样本数量">
                <constraints nullable="false" />
            </column>

            <column name="frozen_box_number" type="integer" remarks="冻存盒数量">
                <constraints nullable="false" />
            </column>

            <column name="empty_tube_number" type="integer" remarks="空管数">
                <constraints nullable="false" />
            </column>

            <column name="empty_hole_number" type="integer" remarks="空孔数">
                <constraints nullable="false" />
            </column>

            <column name="sample_satisfaction" type="integer" remarks="样本满意度">
                <constraints nullable="true" />
            </column>

            <column name="effective_sample_number" type="integer" remarks="有效样本数">
                <constraints nullable="false" />
            </column>

            <column name="tranship_code" type="varchar(255)" remarks="转运编码">
                <constraints nullable="false" />
            </column>

            <column name="project_id" type="bigint" remarks="项目ID">
                <constraints nullable="true" />
            </column>

            <column name="project_site_id" type="bigint" remarks="项目点ID">
                <constraints nullable="true" />
            </column>

            <column name="memo" type="varchar(1024)" remarks="备注">
                <constraints nullable="true" />
            </column>

            <column name="status" type="varchar(20)" remarks="状态">
                <constraints nullable="false" />
            </column>

            <column name="created_by" type="varchar(50)" remarks="创建人">
                <constraints nullable="false"/>
            </column>
            <column name="created_date" type="timestamp" defaultValueDate="${now}" remarks="创建日期">
                <constraints nullable="false"/>
            </column>
            <column name="last_modified_by" type="varchar(50)" remarks="修改人"/>
            <column name="last_modified_date" type="timestamp" remarks="修改日期"/>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>

    </changeSet>
</databaseChangeLog>
