<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <property name="now" value="now()" dbms="h2"/>

    <property name="now" value="sysdate" dbms="oracle"/>
    <property name="autoIncrement" value="true" dbms="mysql,h2,postgresql,oracle,mssql"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle, mssql"/>

    <!--
        Added the entity StockIn.
    -->
    <changeSet id="20170317160203-1" author="jhipster">
        <createTable tableName="stock_in" remarks="入库表">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>

            <column name="stock_in_code" type="varchar(100)" remarks="入库编码">
                <constraints nullable="false" />
            </column>

            <column name="project_code" type="varchar(100)" remarks="项目编码">
                <constraints nullable="false" />
            </column>

            <column name="project_site_code" type="varchar(100)" remarks="项目点编码">
                <constraints nullable="false" />
            </column>

            <column name="receive_date" type="date" remarks="接受日期">
                <constraints nullable="true" />
            </column>

            <column name="receive_id" type="bigint" remarks="接收人ID">
                <constraints nullable="true" />
            </column>

            <column name="receive_name" type="varchar(100)" remarks="接收人姓名">
                <constraints nullable="true" />
            </column>

            <column name="stock_in_type" type="varchar(20)" remarks="入库类型：8001：首次入库，8002：移位入库，8003：调整入库">
                <constraints nullable="false" />
            </column>

            <column name="store_keeper_id_1" type="bigint" remarks="入库人1ID">
                <constraints nullable="true" />
            </column>

            <column name="store_keeper_1" type="varchar(100)" remarks="入库人1名称">
                <constraints nullable="true" />
            </column>

            <column name="store_keeper_id_2" type="bigint" remarks="入库人2ID">
                <constraints nullable="true" />
            </column>

            <column name="store_keeper_2" type="varchar(100)" remarks="入库人2名称">
                <constraints nullable="true" />
            </column>

            <column name="stock_in_date" type="date" remarks="入库时间">
                <constraints nullable="true" />
            </column>

            <column name="count_of_sample" type="integer" remarks="样本数量">
                <constraints nullable="false" />
            </column>

            <column name="sign_id" type="bigint" remarks="签名人ID">
                <constraints nullable="true" />
            </column>

            <column name="sign_name" type="varchar(100)" remarks="签名人名称">
                <constraints nullable="true" />
            </column>

            <column name="sign_date" type="date" remarks="签名日期">
                <constraints nullable="true" />
            </column>

            <column name="tranship_id" type="bigint" remarks="转运ID">
                <constraints nullable="false" />
            </column>

            <column name="project_id" type="bigint" remarks="项目ID">
                <constraints nullable="false" />
            </column>

            <column name="project_site_id" type="bigint" remarks="项目点ID">
                <constraints nullable="false" />
            </column>

            <column name="memo" type="varchar(1024)" remarks="备注">
                <constraints nullable="true" />
            </column>

            <column name="status" type="varchar(20)" remarks="状态">
                <constraints nullable="false" />
            </column>

            <column name="created_by" type="varchar(50)" remarks="创建人">
                <constraints nullable="false"/>
            </column>
            <column name="created_date" type="timestamp" defaultValueDate="${now}" remarks="创建时间">
                <constraints nullable="false"/>
            </column>
            <column name="last_modified_by" type="varchar(50)" remarks="修改人"/>
            <column name="last_modified_date" type="timestamp" remarks="修改时间"/>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>

    </changeSet>
</databaseChangeLog>
