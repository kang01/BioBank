<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <property name="now" value="now()" dbms="h2"/>

    <property name="now" value="sysdate" dbms="oracle"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle, mssql"/>

    <!--
        Added the entity StorageIn.
    -->
    <changeSet id="20170317160203-1" author="jhipster">
        <createTable tableName="storage_in">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="project_code" type="varchar(100)">
                <constraints nullable="false" />
            </column>

            <column name="project_site_code" type="varchar(100)">
                <constraints nullable="false" />
            </column>

            <column name="receive_date" type="date">
                <constraints nullable="false" />
            </column>

            <column name="receive_id" type="bigint">
                <constraints nullable="false" />
            </column>

            <column name="receive_name" type="varchar(100)">
                <constraints nullable="false" />
            </column>

            <column name="storage_in_type" type="varchar(20)">
                <constraints nullable="false" />
            </column>

            <column name="storage_in_person_id_1" type="bigint">
                <constraints nullable="false" />
            </column>

            <column name="storage_in_person_name_1" type="varchar(100)">
                <constraints nullable="false" />
            </column>

            <column name="storage_in_person_id_2" type="bigint">
                <constraints nullable="false" />
            </column>

            <column name="storange_in_person_name_2" type="varchar(100)">
                <constraints nullable="false" />
            </column>

            <column name="storage_in_date" type="date">
                <constraints nullable="false" />
            </column>

            <column name="sample_number" type="integer">
                <constraints nullable="false" />
            </column>

            <column name="sign_id" type="bigint">
                <constraints nullable="false" />
            </column>

            <column name="sign_name" type="varchar(100)">
                <constraints nullable="false" />
            </column>

            <column name="sign_date" type="date">
                <constraints nullable="false" />
            </column>

            <column name="memo" type="varchar(1024)">
                <constraints nullable="true" />
            </column>

            <column name="status" type="varchar(20)">
                <constraints nullable="false" />
            </column>

            <column name="tranship_id" type="bigint">
                <constraints nullable="false" />
            </column>

            <column name="created_by" type="varchar(50)">
                <constraints nullable="false"/>
            </column>
            <column name="created_date" type="timestamp" defaultValueDate="${now}">
                <constraints nullable="false"/>
            </column>
            <column name="last_modified_by" type="varchar(50)"/>
            <column name="last_modified_date" type="timestamp"/>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>
        
    </changeSet>
</databaseChangeLog>
