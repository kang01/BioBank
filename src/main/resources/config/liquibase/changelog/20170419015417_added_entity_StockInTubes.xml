<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <property name="now" value="now()" dbms="h2"/>

    <property name="now" value="sysdate" dbms="oracle"/>
    <property name="autoIncrement" value="true" dbms="mysql,h2,postgresql,oracle,mssql"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle, mssql"/>

    <!--
        Added the entity StockInTubes.
    -->
    <changeSet id="20170419015417-1" author="jhipster">
        <createTable tableName="stock_in_tubes" remarks="入库与冻存管关系表">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="stock_in_code" type="varchar(100)" remarks="入库编码">
                <constraints nullable="false" />
            </column>

            <column name="tranship_code" type="varchar(100)" remarks="转运编码">
                <constraints nullable="true" />
            </column>

            <column name="tranship_batch" type="varchar(100)" remarks="转运批次">
                <constraints nullable="true" />
            </column>

            <column name="frozen_box_code" type="varchar(100)" remarks="冻存盒编码">
                <constraints nullable="false" />
            </column>

            <column name="sample_code" type="varchar(100)" remarks="样本编码">
                <constraints nullable="true" />
            </column>

            <column name="frozen_tube_code" type="varchar(100)" remarks="冻存管编码">
                <constraints nullable="true" />
            </column>

            <column name="rows_in_tube" type="varchar(20)" remarks="行号">
                <constraints nullable="false" />
            </column>

            <column name="columns_in_tube" type="varchar(20)" remarks="列号">
                <constraints nullable="false" />
            </column>

            <column name="memo" type="varchar(1024)" remarks="备注">
                <constraints nullable="true" />
            </column>

            <column name="status" type="varchar(20)" remarks="状态">
                <constraints nullable="false" />
            </column>

            <column name="stock_in_id" type="bigint" remarks="入库ID">
                <constraints nullable="false" />
            </column>

            <column name="tranship_id" type="bigint" remarks="转运ID">
                <constraints nullable="true" />
            </column>

            <column name="frozen_box_id" type="bigint" remarks="冻存盒ID">
                <constraints nullable="false" />
            </column>

            <column name="frozen_tube_id" type="bigint" remarks="冻存管ID">
                <constraints nullable="false" />
            </column>

            <column name="frozen_box_position_id" type="bigint" remarks="冻存盒位置ID">
                <constraints nullable="false" />
            </column>

            <column name="created_by" type="varchar(50)">
                <constraints nullable="false"/>
            </column>
            <column name="created_date" type="timestamp" defaultValueDate="${now}">
                <constraints nullable="false"/>
            </column>
            <column name="last_modified_by" type="varchar(50)"/>
            <column name="last_modified_date" type="timestamp"/>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>

    </changeSet>
</databaseChangeLog>
