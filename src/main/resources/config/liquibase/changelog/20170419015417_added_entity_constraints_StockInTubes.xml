                                 baseTableName="stock_in_tubes"
                                 constraintName="stock_in_tubes_tranship_box_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tranship_box"/>

        <addForeignKeyConstraint baseColumnNames="stock_in_box_id"
                                 baseTableName="stock_in_tubes"
                                 constraintName="stock_in_tubes_stock_in_box_id"
                                 referencedColumnNames="id"
                                 referencedTableName="stock_in_box"/>

    </changeSet>
</databaseChangeLog>
