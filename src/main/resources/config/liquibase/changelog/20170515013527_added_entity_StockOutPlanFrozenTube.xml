<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <property name="now" value="now()" dbms="h2"/>

    <property name="now" value="sysdate" dbms="oracle"/>
    <property name="autoIncrement" value="true" dbms="mysql,h2,postgresql,oracle,mssql"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle, mssql"/>

    <!--
        Added the entity StockOutPlanFrozenTube.
    -->
    <changeSet id="20170515013527-1" author="jhipster">
        <createTable tableName="stock_out_plan_frozen_tube" remarks="出库计划冻存管表">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="tube_rows" type="varchar(100)" remarks="行号">
                <constraints nullable="false" />
            </column>

            <column name="tube_columns" type="varchar(100)" remarks="列号">
                <constraints nullable="false" />
            </column>

            <column name="status" type="varchar(20)" remarks="状态">
                <constraints nullable="false" />
            </column>

            <column name="memo" type="varchar(1024)" remarks="备注">
                <constraints nullable="true" />
            </column>

            <column name="stock_out_plan_id" type="bigint" remarks="出库计划ID">
                <constraints nullable="false" />
            </column>

            <column name="frozen_box_id" type="bigint" remarks="冻存盒ID">
                <constraints nullable="false" />
            </column>

            <column name="frozen_tube_id" type="bigint" remarks="冻存管ID">
                <constraints nullable="false" />
            </column>

            <column name="created_by" type="varchar(50)" remarks="创建人">
                <constraints nullable="false"/>
            </column>
            <column name="created_date" type="timestamp" defaultValueDate="${now}" remarks="创建时间">
                <constraints nullable="false"/>
            </column>
            <column name="last_modified_by" type="varchar(50)" remarks="修改人"/>
            <column name="last_modified_date" type="timestamp" remarks="修改时间"/>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>

    </changeSet>
</databaseChangeLog>
